let getQuestionByDifficultyAndType = (qd, qt) => {
    let shuffled = q.questions.sort( () => 0.5 - Math.random() );
    for(let i = 0; i < shuffled.length; i++) {
        if(shuffled[i].question_difficulty == qd && shuffled[i].question_type == qt) {
            return shuffled[i];
        }
    }  
    return false;  
}

